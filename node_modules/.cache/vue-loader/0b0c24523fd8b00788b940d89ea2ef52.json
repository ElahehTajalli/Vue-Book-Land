n>{{ $t('read') }}</span>\n    </div>\n    <div class=\"read-swiper\" v-if=\"read.length\">\n      <Swiper :books=\"read\" :fromProfile=\"true\" title=\"read\" />\n    </div>\n    <div class=\"my-plan-title\" v-if=\"reading.length\">\n      <span>{{ $t('currently_reading') }}</span>\n    </div>\n    <div class=\"reading-swiper\" v-if=\"reading.length\">\n      <Swiper :books=\"reading\" :fromProfile=\"true\" title=\"currently_reading\" />\n    </div>\n  </div>\n</template>\n\n<script>\n  import { mapGetters, mapActions } from 'vuex'\n  import Swiper from '@/components/Swiper'\n\n  export default {\n    data() {\n      return {\n        read: [],\n        reading: [],\n        wantsToRead: [],\n        favorites: [],\n        loading: false\n      }\n    },\n    async created() {\n      if (this.self) {\n        await this.getPlan()\n        await this.getFavorite()\n      }\n    },\n    computed: {\n      ...mapGetters(['self'])\n    },\n    methods: {\n      ...mapActions(['handleRequest']),\n      getPlan(title) {\n        this.loading = true\n        this.handleRequest({\n          name: 'books/getPlan',\n          action: 'getAll',\n          data: {\n            query: {\n              user: this.self.id,\n              title\n            }\n          }\n        }).then((res) => {\n          this.read = res.data.plan.read\n          this.reading = res.data.plan.reading\n          this.wantsToRead = res.data.plan.wants_to_read\n        })\n      },\n      getFavorite() {\n        this.handleRequest({\n          name: 'books/getFavorite',\n          action: 'getAll',\n          data: {\n            query: {\n              user: this.self.id\n            }\n          }\n        })\n          .then((res) => {\n            this.favorites = res.data.favorite\n          })\n          .finally(() => {\n            this.loading = false\n          })\n      }\n    },\n    watch: {\n      async self() {\n        await this.getPlan()\n        await this.getFavorite()\n      }\n    },\n    components: {\n      Swiper\n    }\n  }\n</script>\n"]}]}