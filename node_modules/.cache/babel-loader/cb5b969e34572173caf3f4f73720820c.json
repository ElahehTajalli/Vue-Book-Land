,eAAA,CAAA,CADA;AAEA,IAAA,cAFA,0BAEA,IAFA,EAEA,IAFA,EAEA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,aAAA,CAAA;AACA,QAAA,IAAA,EAAA,YADA;AAEA,QAAA,MAAA,EAAA,QAFA;AAGA,QAAA,IAAA,EAAA;AACA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA,EADA;AAEA,YAAA,IAAA,EAAA;AAFA;AADA;AAHA,OAAA,EASA,IATA,CASA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,OAXA;AAYA,KAhBA;AAiBA,IAAA,gBAjBA,4BAiBA,IAjBA,EAiBA;AAAA;;AACA,WAAA,aAAA,CAAA;AACA,QAAA,IAAA,EAAA,mBADA;AAEA,QAAA,MAAA,EAAA,QAFA;AAGA,QAAA,IAAA,EAAA;AACA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA,EADA;AAEA,YAAA,IAAA,EAAA;AAFA;AADA;AAHA,OAAA,EASA,IATA,CASA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA,OAXA;AAYA,KA9BA;AA+BA,IAAA,oBA/BA,gCA+BA,IA/BA,EA+BA;AAAA;;AACA,WAAA,aAAA,CAAA;AACA,QAAA,IAAA,EAAA,uBADA;AAEA,QAAA,MAAA,EAAA,QAFA;AAGA,QAAA,IAAA,EAAA;AACA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA,EADA;AAEA,YAAA,IAAA,EAAA;AAFA;AADA;AAHA,OAAA,EAUA,IAVA,CAUA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,WAAA;AACA,OAZA,EAaA,OAbA,CAaA;AAAA,eAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AAAA,OAbA;AAcA;AA9CA,IAhBA;AAgEA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA;AADA;AAhEA,CAAA","sourcesContent":["<template>\n  <div class=\"dashboard\" v-loading.fullscreen.lock=\"loading\">\n    <div class=\"flexbox justify-end\">\n      <!-- <div class=\"pic\"></div> -->\n      <img class=\"main-logo\" src=\"../assets/images/dashboard.svg\" />\n    </div>\n    <Swiper :books=\"newest\" title=\"newest\" class=\"newest\" />\n    <Swiper :books=\"mostPopular\" title=\"most_popular\" class=\"most_popular\" />\n    <div class=\"full-width flexbox align-center justify-center\">\n      <el-card class=\"popular_month\">\n        <!-- <img class=\"month\" src=\"../assets/images/month.svg\" /> -->\n        <!-- <div class=\"flexbox left\"> -->\n        <div class=\"title-div\">\n          <span class=\"title\">{{ $t('most_popular_in_month') }}</span>\n        </div>\n        <div @click=\"showBook(mostPopular[0].id)\" class=\"main\">\n          <img\n            v-if=\"mostPopular[0].image\"\n            :src=\"'http://ketabland.pythonanywhere.com' + mostPopular[0].image\"\n          />\n          <img v-else src=\"../assets/images/noBook.png\" />\n          <div class=\"flexbox align-center justify-center\">\n            <span\n              class=\"book-title bold-title\"\n              @click=\"showBook(mostPopular[0].id)\"\n              >{{ mostPopular[0].name }}</span\n            >\n            <span class=\"book-author\">{{ mostPopular[0].author.name }}</span>\n            <span class=\"book-publisher\">{{ mostPopular[0].publisher }}</span>\n            <span class=\"book-genre\">{{ mostPopular[0].genre }}</span>\n          </div>\n        </div>\n        <!-- </div> -->\n      </el-card>\n    </div>\n    <!-- <Swiper\n      :books=\"[mostPopular[0]]\"\n      title=\"most_popular\"\n      class=\"popular_month\"\n    /> -->\n    <Swiper\n      :users=\"authors\"\n      title=\"most_popular_authors\"\n      class=\"popular_author\"\n    />\n    <Swiper\n      :users=\"translators\"\n      title=\"most_popular_translators\"\n      class=\"popular_translator\"\n    />\n  </div>\n</template>\n\n<script>\n  import { mapActions } from 'vuex'\n  import Swiper from '@/components/Swiper'\n  export default {\n    data() {\n      return {\n        newest: [],\n        mostPopular: [],\n        authors: [],\n        translators: [],\n        loading: false\n      }\n    },\n    async created() {\n      await this.handleGetBooks('created_at:desc', 'newest')\n      await this.handleGetBooks('rate:desc', 'mostPopular')\n      await this.handleGetAuthors('rate:desc')\n      await this.handleGetTranslators('rate:desc')\n    },\n    methods: {\n      ...mapActions(['handleRequest']),\n      handleGetBooks(sort, list) {\n        this.loading = true\n        this.handleRequest({\n          name: 'books/list',\n          action: 'getAll',\n          data: {\n            query: {\n              count: 10,\n              sort\n            }\n          }\n        }).then((res) => {\n          this[list] = res.data.books\n        })\n      },\n      handleGetAuthors(sort) {\n        this.handleRequest({\n          name: 'users/get_authors',\n          action: 'getAll',\n          data: {\n            query: {\n              count: 10,\n              sort\n            }\n          }\n        }).then((res) => {\n          this.authors = res.data.authors\n        })\n      },\n      handleGetTranslators(sort) {\n        this.handleRequest({\n          name: 'users/get_translators',\n          action: 'getAll',\n          data: {\n            query: {\n              count: 10,\n              sort\n            }\n          }\n        })\n          .then((res) => {\n            this.translators = res.data.translators\n          })\n          .finally(() => (this.loading = false))\n      }\n    },\n    components: {\n      Swiper\n    }\n  }\n</script>\n"],"sourceRoot":"src/components"}]}