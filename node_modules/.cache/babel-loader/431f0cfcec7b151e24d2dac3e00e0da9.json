IAAA,EAAA,OADA;AAEA,MAAA,OAAA,EAAA;AAFA;AATA,GAPA;AAqBA,EAAA,QAAA,kCACA,UAAA,CAAA,CAAA,MAAA,CAAA,CADA;AAEA,IAAA,QAFA,sBAEA;AACA,aAAA,MAAA,CAAA,KAAA,IAAA,CAAA,UAAA,CAAA,CAAA,MAAA,CAAA,KAAA,YAAA,CAAA;AACA;AAJA,IArBA;AA2BA,EAAA,OAAA,kCACA,UAAA,CAAA,CAAA,eAAA,CAAA,CADA;AAEA,IAAA,QAFA,oBAEA,EAFA,EAEA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,UAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,EAAA,EAAA;AAAA;AAAA,OAAA;AACA,KAJA;AAKA,IAAA,QALA,oBAKA,MALA,EAKA;AAAA;;AACA,WAAA,aAAA,CAAA;AACA,QAAA,IAAA,EAAA,YADA;AAEA,QAAA,MAAA,EAAA,OAFA;AAGA,QAAA,IAAA,EAAA;AACA,UAAA,OAAA,EAAA;AACA,YAAA,IAAA,EAAA,KAAA,IAAA,CAAA,EADA;AAEA,YAAA,IAAA,EAAA,KAAA,IAAA,CAAA,EAFA;AAGA,YAAA,MAAA,EAAA;AAHA;AADA;AAHA,OAAA,EAUA,IAVA,CAUA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAZA;AAaA,KAnBA;AAoBA,IAAA,QApBA,oBAoBA,EApBA,EAoBA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,UAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,EAAA,EAAA;AAAA;AAAA,OAAA;AACA,KAtBA;AAuBA,IAAA,QAvBA,oBAuBA,EAvBA,EAuBA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,UAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,EAAA,EAAA;AAAA;AAAA,OAAA;AACA;AA1BA;AA3BA,CAAA","sourcesContent":["<template>\n  <div class=\"full-width flexbox justify-center posts\">\n    <el-card class=\"full-width\">\n      <div class=\"criticism\">\n        <div>\n          <div class=\"top\">\n            <div class=\"detail\">\n              <img\n                class=\"image\"\n                src=\"https://img.icons8.com/cotton/64/000000/person-male--v2.png\"\n              />\n              <span v-if=\"isMyPosts\" @click=\"showBook(post.book.id)\">\n                {{ post.book.name }}\n              </span>\n              <span v-else @click=\"showUser(post.critic.id)\">\n                {{ post.critic.persian_username }}</span\n              >\n            </div>\n            <el-button @click=\"showPost(post.id)\" type=\"primary\">\n              {{ $t('view') }}\n            </el-button>\n          </div>\n          <span class=\"text\">{{ post.text }}</span>\n        </div>\n        <div class=\"bottom\">\n          <div class=\"date\">\n            <img src=\"https://img.icons8.com/ios/100/184277/planner.png\" />\n            <span>{{ dateTime }}</span>\n          </div>\n          <div class=\"likes\">\n            <div @click=\"editPost('dislike')\" v-if=\"new_post.dislikes\">\n              <span> {{ new_post.dislikes.length }} </span>\n              <img\n                src=\"https://img.icons8.com/material-rounded/24/ff0000/thumbs-down.png\"\n              />\n            </div>\n            <div @click=\"editPost('like')\" v-if=\"new_post.likes\">\n              <span> {{ new_post.likes.length }} </span>\n              <img\n                src=\"https://img.icons8.com/material-rounded/24/00ff30/facebook-like--v1.png\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </el-card>\n    <el-card class=\"information-book\" v-if=\"fromHome\">\n      <div class=\"flexbox column-direction detail\">\n        <span class=\"name bold-title\"> {{ post.book.name }} </span>\n        <span v-if=\"post.book.author\">\n          {{ $t('book.author') }}: {{ post.book.author.name }}\n        </span>\n        <span v-if=\"post.book.translator\">\n          {{ $t('book.translator') }}: {{ post.book.translator.name }}\n        </span>\n        <span> {{ $t('book.publisher') }}: {{ post.book.publisher }} </span>\n      </div>\n      <img\n        v-if=\"post.book.image\"\n        :src=\"'http://127.0.0.1:8000' + post.book.image\"\n      />\n      <img v-else src=\"../../assets/images/noBook.png\" />\n    </el-card>\n  </div>\n</template>\n\n<script>\n  import { mapActions, mapGetters } from 'vuex'\n  import moment from 'moment-jalaali'\n  export default {\n    data() {\n      return {\n        jalaliFormat: 'jYYYY/jM/jD',\n        new_post: this.post\n      }\n    },\n    props: {\n      post: {\n        type: Object,\n        default: () => {}\n      },\n      isMyPosts: {\n        type: Boolean,\n        default: false\n      },\n      fromHome: {\n        type: Boolean,\n        default: false\n      }\n    },\n    computed: {\n      ...mapGetters(['self']),\n      dateTime() {\n        return moment(this.post.created_at).format(this.jalaliFormat)\n      }\n    },\n    methods: {\n      ...mapActions(['handleRequest']),\n      showPost(id) {\n        this.$router.push({ name: 'ViewPost', params: { id } })\n      },\n      editPost(action) {\n        this.handleRequest({\n          name: 'posts/edit',\n          action: 'patch',\n          data: {\n            toPatch: {\n              user: this.self.id,\n              post: this.post.id,\n              action\n            }\n          }\n        }).then((res) => {\n          this.new_post = res.data.post\n        })\n      },\n      showUser(id) {\n        this.$router.push({ name: 'ViewUser', params: { id } })\n      },\n      showBook(id) {\n        console.log(id)\n        this.$router.push({ name: 'ViewBook', params: { id } })\n      }\n    }\n  }\n</script>\n"],"sourceRoot":"src/components/post"}]}