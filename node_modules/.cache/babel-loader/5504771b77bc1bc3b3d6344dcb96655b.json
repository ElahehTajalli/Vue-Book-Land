ACA;AACA;AACA,KAzEA;AA0EA,IAAA,cA1EA,0BA0EA,CA1EA,EA0EA;AACA,UAAA,GAAA,GAAA,CAAA,CAAA,GAAA;;AACA,UAAA,GAAA,KAAA,OAAA,EAAA;AACA,QAAA,CAAA,CAAA,cAAA;AACA,aAAA,qBAAA;AACA;AACA,KAhFA;AAiFA,IAAA,WAjFA,uBAiFA,IAjFA,EAiFA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KAnFA;AAoFA,IAAA,WApFA,yBAoFA;AACA,WAAA,UAAA,GAAA,CAAA;AACA,WAAA,KAAA,CAAA,eAAA,EAAA,WAAA;AACA,WAAA,KAAA,CAAA,sBAAA,EAAA,WAAA;AACA,WAAA,KAAA,CAAA,YAAA,EAAA,WAAA;AACA;AAzFA,IA1HA;AAqNA,EAAA,OArNA,qBAqNA;AACA,IAAA,QAAA,CAAA,gBAAA,CAAA,UAAA,EAAA,KAAA,cAAA;AACA,GAvNA;AAwNA,EAAA,aAxNA,2BAwNA;AACA,IAAA,QAAA,CAAA,mBAAA,CAAA,UAAA,EAAA,KAAA,cAAA;AACA;AA1NA,CAAA","sourcesContent":["<template>\n  <div class=\"signup\">\n    <div class=\"logo text-center mb-10\" @click=\"changeRoute('Dashboard')\">\n      {{ $t('book_land') }}\n    </div>\n    <el-card>\n      <div slot=\"header\" class=\"clearfix\">\n        <span>{{ $t('signup') }}</span>\n      </div>\n      <div>\n        <el-steps\n          :active=\"activeStep\"\n          align-center\n          finish-status=\"success\"\n          class=\"mb-15 text-right\"\n          style=\"margin-bottom:30px;\"\n        >\n          <el-step :title=\"$t('insert_email')\"></el-step>\n          <el-step :title=\"$t('verification_code')\"></el-step>\n          <el-step :title=\"$t('insert_info')\"></el-step>\n        </el-steps>\n        <el-form\n          v-show=\"activeStep === 0\"\n          :model=\"sendEmailForm\"\n          ref=\"sendEmailForm\"\n          label-position=\"top\"\n          :disabled=\"loading\"\n        >\n          <el-form-item\n            prop=\"email\"\n            :rules=\"[{ validator: checkEmail, trigger: 'blur' }]\"\n          >\n            <el-input\n              autocomplete=\"new-email\"\n              class=\"ltr text-right\"\n              :placeholder=\"$t('email')\"\n              v-model=\"sendEmailForm.email\"\n            >\n            </el-input>\n          </el-form-item>\n          <el-form-item class=\"flexbox align-center justify-center no_margin\">\n            <el-button\n              type=\"primary\"\n              @click=\"handleRegisterFormSubmit\"\n              :loading=\"loading\"\n            >\n              {{ $t('next_step') }}\n            </el-button>\n          </el-form-item>\n          <div class=\"text-center buttons_div\">\n            <el-button type=\"text\" @click=\"changeRoute('Dashboard')\">\n              {{ $t('main_page') }}\n            </el-button>\n            |\n            <el-button\n              type=\"text\"\n              @click=\"changeRoute('Login')\"\n              class=\"no_margin\"\n            >\n              {{ $t('login') }}\n            </el-button>\n          </div>\n        </el-form>\n\n        <el-form\n          v-show=\"activeStep === 1\"\n          :model=\"verificationCodeForm\"\n          ref=\"verificationCodeForm\"\n          label-position=\"top\"\n          :disabled=\"loading\"\n        >\n          <el-form-item\n            prop=\"code\"\n            :rules=\"[{ message: $t('insert_your_code'), trigger: 'blur' }]\"\n          >\n            <small>\n              {{\n                $t('please_enter_verification_code_that_is_sent_to_email', {\n                  email: sendEmailForm.email\n                })\n              }}\n            </small>\n            <el-input\n              class=\"ltr text-right mt-10\"\n              :placeholder=\"$t('verification_code')\"\n              v-model=\"verificationCodeForm.code\"\n            >\n            </el-input>\n          </el-form-item>\n          <el-form-item class=\"flexbox align-center justify-center no_margin\">\n            <el-button\n              type=\"primary\"\n              @click=\"handleRegisterFormSubmit\"\n              :loading=\"loading\"\n            >\n              {{ $t('next_step') }}\n            </el-button>\n            <el-button @click=\"goToStepOne\" :loading=\"loading\" class=\"mt-15\">\n              {{ $t('prev_step') }}\n            </el-button>\n          </el-form-item>\n        </el-form>\n\n        <el-form\n          v-show=\"activeStep === 2\"\n          :model=\"signUpForm\"\n          :rules=\"rules\"\n          ref=\"signUpForm\"\n          label-position=\"top\"\n          :disabled=\"loading\"\n        >\n          <el-form-item prop=\"name\">\n            <el-input\n              class=\"ltr text-right\"\n              :placeholder=\"$t('name')\"\n              v-model=\"signUpForm.name\"\n            >\n            </el-input>\n          </el-form-item>\n          <el-form-item prop=\"username\">\n            <el-input\n              autocomplete=\"new-username\"\n              class=\"ltr text-right\"\n              :placeholder=\"$t('username')\"\n              v-model=\"signUpForm.username\"\n            >\n            </el-input>\n          </el-form-item>\n          <el-form-item prop=\"password\">\n            <el-input\n              autocomplete=\"new-password\"\n              type=\"password\"\n              class=\"ltr text-right\"\n              @focus=\"isShow = true\"\n              @blur=\"isShow = false\"\n              v-model=\"signUpForm.password\"\n              :placeholder=\"$t('password')\"\n            ></el-input>\n            <div v-if=\"isShow\" class=\"password-validation\">\n              <el-checkbox v-model=\"passLength\" disabled>{{\n                $t('eight_characters_or_longer')\n              }}</el-checkbox>\n              <el-checkbox v-model=\"latinPass\" disabled>{{\n                $t('contain_at_least_one_alphabetical_character')\n              }}</el-checkbox>\n              <el-checkbox v-model=\"passNumber\" disabled>{{\n                $t('contain_at_least_one_numeric_character')\n              }}</el-checkbox>\n              <el-checkbox v-model=\"persianPass\" disabled>{{\n                $t('use_english_character_and_numbers')\n              }}</el-checkbox>\n            </div>\n          </el-form-item>\n          <el-form-item prop=\"confirm_password\">\n            <el-input\n              class=\"ltr text-right\"\n              :placeholder=\"$t('confirm_password')\"\n              v-model=\"signUpForm.confirm_password\"\n            >\n            </el-input>\n          </el-form-item>\n          <el-form-item class=\"flexbox align-center justify-center no_margin\">\n            <el-button\n              type=\"primary\"\n              @click=\"handleRegisterFormSubmit\"\n              :loading=\"loading\"\n            >\n              {{ $t('signup') }}\n            </el-button>\n            <el-button @click=\"goToStepOne\" :loading=\"loading\" class=\"mt-15\">\n              {{ $t('reset') }}\n            </el-button>\n          </el-form-item>\n        </el-form>\n      </div>\n    </el-card>\n  </div>\n</template>\n\n<script>\n  import { mapActions, mapGetters } from 'vuex'\n  import {\n    passwordLength,\n    latinPassword,\n    passwordNumber,\n    persianPassword,\n    correctPassword,\n    email\n  } from '@/utils/patterns'\n  export default {\n    data() {\n      const getSendEmailForm = () => {\n        return {\n          email: ''\n        }\n      }\n      const getVerificationCodeForm = () => {\n        return {\n          code: ''\n        }\n      }\n      const getSignUpForm = () => {\n        return {\n          name: '',\n          username: '',\n          password: '',\n          confirm_password: ''\n        }\n      }\n      const checkPassword = (rule, value, callback) => {\n        this.resetPasswordValidation()\n        if (value === '') {\n          callback(new Error(this.$i18n.t('insert_your_password')))\n        } else if (\n          !correctPassword.test(value) ||\n          persianPassword.test(value)\n        ) {\n          if (passwordLength.test(value)) {\n            this.passLength = true\n            callback(new Error(this.$i18n.t('wrong_password')))\n          }\n          if (latinPassword.test(value)) {\n            this.latinPass = true\n            callback(new Error(this.$i18n.t('wrong_password')))\n          }\n          if (passwordNumber.test(value)) {\n            this.passNumber = true\n            callback(new Error(this.$i18n.t('wrong_password')))\n          }\n          if (!persianPassword.test(value) && value.length) {\n            this.persianPass = true\n            callback(new Error(this.$i18n.t('wrong_password')))\n          }\n        } else {\n          this.setPasswordValidation()\n          callback()\n        }\n      }\n      const checkConfirmPassword = (rule, value, callback) => {\n        if (value === '') {\n          callback(\n            new Error(this.$i18n.t('please_insert_password_confirmation'))\n          )\n        } else if (value !== this.userForm.password) {\n          callback(new Error(this.$i18n.t('passwords_do_not_match')))\n        } else {\n          callback()\n        }\n      }\n      return {\n        loading: false,\n        loginForm: {\n          username: '',\n          password: ''\n        },\n        rules: {\n          name: [\n            {\n              message: this.$i18n.t('insert_your_name'),\n              required: true,\n              trigger: 'blur'\n            }\n          ],\n          username: [\n            {\n              message: this.$i18n.t('insert_your_username'),\n              required: true,\n              trigger: 'blur'\n            }\n          ],\n          password: [\n            {\n              validator: checkPassword,\n              required: true\n            }\n          ],\n          confirm_password: [\n            {\n              validator: checkConfirmPassword,\n              required: true,\n              trigger: 'blur'\n            }\n          ]\n        },\n        activeStep: 0,\n        forms: ['sendEmailForm', 'verificationCodeForm', 'signUpForm'],\n        callbacks: [\n          this.handleSendEmail,\n          this.handleCheckVerificationCode,\n          this.handleSignUp\n        ],\n        sendEmailForm: getSendEmailForm(),\n        verificationCodeForm: getVerificationCodeForm(),\n        signUpForm: getSignUpForm(),\n        isShow: false,\n        passLength: false,\n        latinPass: false,\n        passNumber: false,\n        persianPass: false,\n        isEmailExists: false\n      }\n    },\n    computed: {\n      ...mapGetters(['user']),\n      currentForm() {\n        return this.forms[this.activeStep]\n      },\n      currentCallback() {\n        return this.callbacks[this.activeStep]\n      }\n    },\n    methods: {\n      ...mapActions(['handleRequest']),\n      disableLoading() {\n        this.loading = false\n      },\n      checkEmail(rule, value, callback) {\n        if (!value.length) {\n          callback(new Error(this.$i18n.t('insert_your_email')))\n        } else if (!email.test(value)) {\n          callback(new Error(this.$i18n.t('insert_valid_email')))\n        } else {\n          callback()\n        }\n      },\n      resetPasswordValidation() {\n        this.passLength = false\n        this.latinPass = false\n        this.passNumber = false\n        this.persianPass = false\n      },\n      setPasswordValidation() {\n        this.passLength = true\n        this.latinPass = true\n        this.passNumber = true\n        this.persianPass = true\n      },\n      handleRegisterFormSubmit() {\n        let { currentForm, currentCallback } = this\n        this.$refs[currentForm].validate((valid) => {\n          if (valid) {\n            if (this.activeStep === 1) {\n              this.$refs['signUpForm'].resetFields()\n              this.resetPasswordValidation()\n            }\n            this.loading = true\n            currentCallback()\n          } else {\n            return false\n          }\n        })\n      },\n      handleCheckVerificationCode() {\n        this.activeStep = 2\n        this.loading = false\n      },\n      handleSendEmail() {\n        this.activeStep = 1\n        this.loading = false\n      },\n      handleLoginForm() {\n        let formData = new FormData()\n        formData.append('email', this.loginForm.username)\n        // this.handleRequest({\n        //   name: 'password-reset/',\n        //   action: 'create',\n        //   data: formData\n        // })\n        //   .then(() => {\n        //     this.disableLoading()\n        //     this.$message({\n        //       type: 'success',\n        //       message: this.$i18n.t('send_reset_link_please_check_your_email'),\n        //       customClass: 'bg-success-dark'\n        //     })\n        //   })\n        //   .catch(() => {\n        //     this.$message({\n        //       type: 'error',\n        //       message: this.$i18n.t('email_not_exist'),\n        //       customClass: 'bg-error-dark'\n        //     })\n        //     this.disableLoading()\n        //   })\n      },\n      handleKeyPress(e) {\n        let { key } = e\n        if (key === 'Enter') {\n          e.preventDefault()\n          this.handleLoginFormSubmit()\n        }\n      },\n      changeRoute(name) {\n        this.$router.push({ name })\n      },\n      goToStepOne() {\n        this.activeStep = 0\n        this.$refs['sendEmailForm'].resetFields()\n        this.$refs['verificationCodeForm'].resetFields()\n        this.$refs['signUpForm'].resetFields()\n      }\n    },\n    created() {\n      document.addEventListener('keypress', this.handleKeyPress)\n    },\n    beforeDestroy() {\n      document.removeEventListener('keypress', this.handleKeyPress)\n    }\n  }\n</script>\n"],"sourceRoot":"src/components/auth"}]}