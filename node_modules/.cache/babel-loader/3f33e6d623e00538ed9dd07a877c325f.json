A,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,OAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,aAAA;AACA,OAbA;AAcA,KAlBA;AAmBA,IAAA,WAnBA,yBAmBA;AAAA;;AACA,WAAA,aAAA,CAAA;AACA,QAAA,IAAA,EAAA,mBADA;AAEA,QAAA,MAAA,EAAA,QAFA;AAGA,QAAA,IAAA,EAAA;AACA,UAAA,KAAA,EAAA;AACA,YAAA,IAAA,EAAA,KAAA,IAAA,CAAA;AADA;AADA;AAHA,OAAA,EASA,IATA,CASA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,QAAA;AACA,OAXA,EAYA,OAZA,CAYA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,OAdA;AAeA;AAnCA,IAnBA;AAwDA,EAAA,KAAA,EAAA;AACA,IAAA,IADA,kBACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,OAAA,EADA;;AAAA;AAAA;AAAA,uBAEA,MAAA,CAAA,WAAA,EAFA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA;AAJA,GAxDA;AA8DA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA;AADA;AA9DA,CAAA","sourcesContent":["<template>\n  <div class=\"user-profile my-library\" v-loading.fullscreen.lock=\"loading\">\n    <div class=\"my-plan-title\" v-if=\"favorites.length\">\n      <span>{{ $t('favorites') }}</span>\n    </div>\n    <div class=\"favorite\" v-if=\"favorites.length\">\n      <Swiper :books=\"favorites\" :fromProfile=\"true\" title=\"favorites\" />\n    </div>\n    <div class=\"my-plan-title\" v-if=\"wantsToRead.length\">\n      <span>{{ $t('want_to_read') }}</span>\n    </div>\n    <div class=\"wants-to-read-swiper\" v-if=\"wantsToRead.length\">\n      <Swiper :books=\"wantsToRead\" :fromProfile=\"true\" title=\"want_to_read\" />\n    </div>\n    <div class=\"my-plan-title\" v-if=\"read.length\">\n      <span>{{ $t('read') }}</span>\n    </div>\n    <div class=\"read-swiper\" v-if=\"read.length\">\n      <Swiper :books=\"read\" :fromProfile=\"true\" title=\"read\" />\n    </div>\n    <div class=\"my-plan-title\" v-if=\"reading.length\">\n      <span>{{ $t('currently_reading') }}</span>\n    </div>\n    <div class=\"reading-swiper\" v-if=\"reading.length\">\n      <Swiper :books=\"reading\" :fromProfile=\"true\" title=\"currently_reading\" />\n    </div>\n  </div>\n</template>\n\n<script>\n  import { mapGetters, mapActions } from 'vuex'\n  import Swiper from '@/components/Swiper'\n\n  export default {\n    data() {\n      return {\n        read: [],\n        reading: [],\n        wantsToRead: [],\n        favorites: [],\n        loading: false\n      }\n    },\n    async created() {\n      if (this.self) {\n        await this.getPlan()\n        await this.getFavorite()\n      }\n    },\n    computed: {\n      ...mapGetters(['self'])\n    },\n    methods: {\n      ...mapActions(['handleRequest']),\n      getPlan(title) {\n        this.loading = true\n        this.handleRequest({\n          name: 'books/getPlan',\n          action: 'getAll',\n          data: {\n            query: {\n              user: this.self.id,\n              title\n            }\n          }\n        }).then((res) => {\n          this.read = res.data.plan.read\n          this.reading = res.data.plan.reading\n          this.wantsToRead = res.data.plan.wants_to_read\n        })\n      },\n      getFavorite() {\n        this.handleRequest({\n          name: 'books/getFavorite',\n          action: 'getAll',\n          data: {\n            query: {\n              user: this.self.id\n            }\n          }\n        })\n          .then((res) => {\n            this.favorites = res.data.favorite\n          })\n          .finally(() => {\n            this.loading = false\n          })\n      }\n    },\n    watch: {\n      async self() {\n        await this.getPlan()\n        await this.getFavorite()\n      }\n    },\n    components: {\n      Swiper\n    }\n  }\n</script>\n"],"sourceRoot":"src/components"}]}