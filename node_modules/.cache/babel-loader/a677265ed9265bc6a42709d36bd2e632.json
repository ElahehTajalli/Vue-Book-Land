flexbox justify-center column-direction\"\n    v-loading.fullscreen.lock=\"loading\"\n  >\n    <Filters @set-filter=\"setFilter($event)\" />\n    <div class=\"items\">\n      <Detail v-for=\"b in books\" :key=\"b.id\" :book=\"b\" />\n    </div>\n  </div>\n</template>\n\n<script>\n  import { mapActions } from 'vuex'\n  import Detail from '@/components/book/Detail'\n  import Filters from '@/components/book/Filters'\n  export default {\n    data() {\n      return {\n        books: [],\n        loading: false\n      }\n    },\n    created() {\n      this.handleGetBooks()\n    },\n    methods: {\n      ...mapActions(['handleRequest']),\n      handleGetBooks() {\n        this.loading = true\n        this.handleRequest({\n          name: 'books/list',\n          action: 'getAll',\n          data: {\n            query: {\n              count: 10,\n              sort: 'created_at:desc'\n            }\n          }\n        })\n          .then((res) => {\n            this.books = res.data.books\n          })\n          .finally(() => {\n            this.loading = false\n          })\n      },\n      setFilter(books) {\n        this.books = books\n      }\n    },\n    components: {\n      Detail,\n      Filters\n    }\n  }\n</script>\n"],"sourceRoot":"src/components/book"}]}