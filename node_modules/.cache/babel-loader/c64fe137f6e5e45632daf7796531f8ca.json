AA;AACA,OAXA;AAYA,KA7BA;AA8BA,IAAA,oBA9BA,gCA8BA,IA9BA,EA8BA;AAAA;;AACA,WAAA,aAAA,CAAA;AACA,QAAA,IAAA,EAAA,uBADA;AAEA,QAAA,MAAA,EAAA,QAFA;AAGA,QAAA,IAAA,EAAA;AACA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA,EADA;AAEA,YAAA,IAAA,EAAA;AAFA;AADA;AAHA,OAAA,EASA,IATA,CASA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,WAAA;AACA,OAXA;AAYA;AA3CA,IATA;AAsDA,EAAA,OAtDA,qBAsDA;AACA,SAAA,cAAA,CAAA,iBAAA,EAAA,QAAA;AACA,SAAA,cAAA,CAAA,WAAA,EAAA,aAAA;AACA,SAAA,gBAAA,CAAA,WAAA;AACA,SAAA,oBAAA,CAAA,WAAA;AACA,GA3DA;AA4DA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA;AADA;AA5DA,CAAA","sourcesContent":["<template>\n  <div>\n    <div>\n      <div class=\"pic\"></div>\n    </div>\n    <Swiper :books=\"newest\" title=\"newest\" class=\"newest\" />\n    <Swiper :books=\"mostPopular\" title=\"most_popular\" class=\"most_popular\" />\n    <Swiper :users=\"authors\" title=\"most_popular_authors\" class=\"newest\" />\n    <Swiper\n      :users=\"translators\"\n      title=\"most_popular_translators\"\n      class=\"most_popular\"\n    />\n  </div>\n</template>\n\n<script>\n  import { mapActions } from 'vuex'\n  import Swiper from '@/components/Swiper'\n  export default {\n    data() {\n      return {\n        newest: [],\n        mostPopular: [],\n        authors: [],\n        translators: []\n      }\n    },\n    methods: {\n      ...mapActions(['handleRequest']),\n      handleGetBooks(sort, list) {\n        this.handleRequest({\n          name: 'books/get',\n          action: 'getAll',\n          data: {\n            query: {\n              count: 10,\n              sort\n            }\n          }\n        }).then((res) => {\n          this[list] = res.data.books\n        })\n      },\n      handleGetAuthors(sort) {\n        this.handleRequest({\n          name: 'users/get_authors',\n          action: 'getAll',\n          data: {\n            query: {\n              count: 10,\n              sort\n            }\n          }\n        }).then((res) => {\n          this.authors = res.data.authors\n        })\n      },\n      handleGetTranslators(sort) {\n        this.handleRequest({\n          name: 'users/get_translators',\n          action: 'getAll',\n          data: {\n            query: {\n              count: 10,\n              sort\n            }\n          }\n        }).then((res) => {\n          this.translators = res.data.translators\n        })\n      }\n    },\n    created() {\n      this.handleGetBooks('created_at:desc', 'newest')\n      this.handleGetBooks('rate:desc', 'mostPopular')\n      this.handleGetAuthors('rate:desc')\n      this.handleGetTranslators('rate:desc')\n    },\n    components: {\n      Swiper\n    }\n  }\n</script>\n"],"sourceRoot":"src/components"}]}