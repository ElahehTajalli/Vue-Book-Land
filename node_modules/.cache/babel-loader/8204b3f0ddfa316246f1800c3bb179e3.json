ACA,WAAA,OAAA,GAAA,IAAA;;AACA,UAAA,OAAA,6EACA,KAAA,OAAA,CAAA,OAAA,CAAA,MAAA,IAAA;AAAA,QAAA,OAAA,EAAA,KAAA,OAAA,CAAA;AAAA,OADA,GAEA,KAAA,OAAA,CAAA,WAAA,CAAA,MAAA,IAAA;AACA,QAAA,WAAA,EAAA,KAAA,OAAA,CAAA;AADA,OAFA,GAKA,KAAA,OAAA,CAAA,KAAA,IAAA;AAAA,QAAA,KAAA,EAAA,KAAA,OAAA,CAAA;AAAA,OALA,GAMA,KAAA,OAAA,CAAA,SAAA,IAAA;AAAA,QAAA,SAAA,EAAA,KAAA,OAAA,CAAA;AAAA,OANA;AAOA,QAAA,IAAA,EAAA,KAAA,OAAA,CAAA;AAPA,QAAA;;AAUA,WAAA,aAAA,CAAA;AACA,QAAA,IAAA,EAAA,YADA;AAEA,QAAA,MAAA,EAAA,QAFA;AAGA,QAAA,IAAA,EAAA;AACA,UAAA,KAAA,EAAA;AADA;AAHA,OAAA,EAOA,IAPA,CAOA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,YAAA,EAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,OATA,EAUA,OAVA,CAUA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,OAZA;AAaA,KA3BA;AA4BA,IAAA,eA5BA,6BA4BA;AACA,WAAA,OAAA,GAAA;AACA,QAAA,OAAA,EAAA,EADA;AAEA,QAAA,WAAA,EAAA,EAFA;AAGA,QAAA,KAAA,EAAA,EAHA;AAIA,QAAA,SAAA,EAAA,EAJA;AAKA,QAAA,IAAA,EAAA;AALA,OAAA;AAOA,KApCA;AAqCA,IAAA,WArCA,yBAqCA;AACA,WAAA,eAAA;AACA,WAAA,YAAA;AACA,KAxCA;AAyCA,IAAA,gBAzCA,4BAyCA,IAzCA,EAyCA;AAAA;;AACA,WAAA,aAAA,CAAA;AACA,QAAA,IAAA,EAAA,mBADA;AAEA,QAAA,MAAA,EAAA,QAFA;AAGA,QAAA,IAAA,EAAA;AACA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA,EADA;AAEA,YAAA,IAAA,EAAA;AAFA;AADA;AAHA,OAAA,EASA,IATA,CASA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA,OAXA;AAYA,KAtDA;AAuDA,IAAA,oBAvDA,gCAuDA,IAvDA,EAuDA;AAAA;;AACA,WAAA,aAAA,CAAA;AACA,QAAA,IAAA,EAAA,uBADA;AAEA,QAAA,MAAA,EAAA,QAFA;AAGA,QAAA,IAAA,EAAA;AACA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA,EADA;AAEA,YAAA,IAAA,EAAA;AAFA;AADA;AAHA,OAAA,EAUA,IAVA,CAUA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,WAAA;AACA,OAZA,EAaA,OAbA,CAaA;AAAA,eAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AAAA,OAbA;AAcA;AAtEA,IAnBA;AA2FA,EAAA,SA3FA,uBA2FA;AACA,SAAA,WAAA;AACA;AA7FA,CAAA","sourcesContent":["<template>\n  <el-card v-loading.fullscreen.lock=\"loading\" class=\"filters\">\n    <el-form ref=\"filters\" :model=\"filters\">\n      <div class=\"filter-item\">\n        <el-form-item :label=\"$t('book.author')\">\n          <el-select\n            v-model=\"filters.authors\"\n            multiple\n            default-first-option\n            filterable\n          >\n            <el-option\n              v-for=\"item in authors\"\n              :key=\"item.id\"\n              :label=\"item.name\"\n              :value=\"item.name\"\n            >\n            </el-option>\n          </el-select>\n        </el-form-item>\n        <el-form-item :label=\"$t('book.translator')\">\n          <el-select\n            v-model=\"filters.translators\"\n            multiple\n            default-first-option\n            filterable\n          >\n            <el-option\n              v-for=\"item in translators\"\n              :key=\"item.id\"\n              :label=\"item.name\"\n              :value=\"item.name\"\n            >\n            </el-option>\n          </el-select>\n        </el-form-item>\n        <el-form-item :label=\"$t('book.genre')\">\n          <el-input v-model=\"filters.genre\" />\n        </el-form-item>\n        <el-form-item :label=\"$t('book.publisher')\">\n          <el-input v-model=\"filters.publisher\" />\n        </el-form-item>\n      </div>\n      <div class=\"text-center\">\n        <el-button type=\"primary\" @click=\"handleFilter\">{{\n          $t('filter')\n        }}</el-button>\n        <el-button class=\"mr-5\" @click=\"resetFilter\">{{\n          $t('reset_filter')\n        }}</el-button>\n      </div>\n    </el-form>\n  </el-card>\n</template>\n\n<script>\n  import { mapActions } from 'vuex'\n  export default {\n    data() {\n      return {\n        filters: {\n          authors: [],\n          translators: [],\n          genre: '',\n          publisher: '',\n          sort: 'created_at:desc'\n        },\n        loading: false,\n        authors: [],\n        translators: []\n      }\n    },\n    async created() {\n      await this.handleGetAuthors('rate:desc')\n      await this.handleGetTranslators('rate:desc')\n    },\n    methods: {\n      ...mapActions(['handleRequest']),\n      handleFilter() {\n        this.loading = true\n        let filters = {\n          ...(this.filters.authors.length && { authors: this.filters.authors }),\n          ...(this.filters.translators.length && {\n            translators: this.filters.translators\n          }),\n          ...(this.filters.genre && { genre: this.filters.genre }),\n          ...(this.filters.publisher && { publisher: this.filters.publisher }),\n          sort: this.filters.sort\n        }\n\n        this.handleRequest({\n          name: 'books/list',\n          action: 'getAll',\n          data: {\n            query: filters\n          }\n        })\n          .then((res) => {\n            this.$emit('set-filter', res.data.books)\n          })\n          .finally(() => {\n            this.loading = false\n          })\n      },\n      setFiltersEmpty() {\n        this.filters = {\n          authors: [],\n          translators: [],\n          genre: '',\n          publisher: '',\n          sort: 'created_at:desc'\n        }\n      },\n      resetFilter() {\n        this.setFiltersEmpty()\n        this.handleFilter()\n      },\n      handleGetAuthors(sort) {\n        this.handleRequest({\n          name: 'users/get_authors',\n          action: 'getAll',\n          data: {\n            query: {\n              count: 10,\n              sort\n            }\n          }\n        }).then((res) => {\n          this.authors = res.data.authors\n        })\n      },\n      handleGetTranslators(sort) {\n        this.handleRequest({\n          name: 'users/get_translators',\n          action: 'getAll',\n          data: {\n            query: {\n              count: 10,\n              sort\n            }\n          }\n        })\n          .then((res) => {\n            this.translators = res.data.translators\n          })\n          .finally(() => (this.loading = false))\n      }\n    },\n    destroyed() {\n      this.resetFilter()\n    }\n  }\n</script>\n"],"sourceRoot":"src/components/book"}]}