rt, list) {\n        this.loading = true\n        this.handleRequest({\n          name: 'books/list',\n          action: 'getAll',\n          data: {\n            query: {\n              count: 10,\n              sort\n            }\n          }\n        }).then((res) => {\n          this[list] = res.data.books\n        })\n      },\n      handleGetAuthors(sort) {\n        this.handleRequest({\n          name: 'users/get_authors',\n          action: 'getAll',\n          data: {\n            query: {\n              count: 10,\n              sort\n            }\n          }\n        }).then((res) => {\n          this.authors = res.data.authors\n        })\n      },\n      handleGetTranslators(sort) {\n        this.handleRequest({\n          name: 'users/get_translators',\n          action: 'getAll',\n          data: {\n            query: {\n              count: 10,\n              sort\n            }\n          }\n        })\n          .then((res) => {\n            this.translators = res.data.translators\n          })\n          .finally(() => (this.loading = false))\n      }\n    },\n    components: {\n      Swiper\n    }\n  }\n</script>\n"],"sourceRoot":"src/components"}]}